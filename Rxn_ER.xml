  <Specie name="IP3RIP3"          id="IP3RIP3"          kdiff="0"     kdiffunit="mu2/s"/>
  <Specie name="IP3RO"           id="IP3RO"           kdiff="0"     kdiffunit="mu2/s"/>
  <Specie name="IP3RC"           id="IP3RC"           kdiff="0"     kdiffunit="mu2/s"/>
