<?xml version="1.0"?>
<ReactionScheme>
  <Specie name="Ca" id="Ca" kdiff="100" kdiffunit="mu2/s" />
  <Specie name="CaER" id="CaER" kdiff="10." kdiffunit="mu2/s"/>
  <Specie name="RyRD2D3"            id="RyRD2D3"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRD24CaMD3"            id="RyRD24CaMD3"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMD2D3"            id="RyR4CaMD2D3"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMCCa8D2D3"            id="RyR4CaMCCa8D2D3"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMCa16D2D3"            id="RyR4CaMCa16D2D3"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca4RyR4CaMCCa8D2D3"            id="Ca4RyR4CaMCCa8D2D3"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca4RyR4CaMCa16D2D3"            id="Ca4RyR4CaMCa16D2D3"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRO1"            id="RyRO1"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRO2"            id="RyRO2"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMCa16D2D3clamped"            id="RyR4CaMCa16D2D3clamped"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca4RyR4CaMCa16D2D3clamped"            id="Ca4RyR4CaMCa16D2D3clamped"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca8RyR4CaMCa16D2D3clamped"            id="Ca8RyR4CaMCa16D2D3clamped"           kdiff="0" kdiffunit="mu2/s"/>
  
  <Specie name="CaM" id="CaM" kdiff="4" kdiffunit="mu2/s" />
  <!--Calmodulin-->
  <Specie name="CaMCa2C" id="CaMCa2C" kdiff="4" kdiffunit="mu2/s" />
  <Specie name="CaMCa2N" id="CaMCa2N" kdiff="4" kdiffunit="mu2/s" />
  <!-- 2 Ca bound Calmodulin-->
  <Specie name="CaMCa4" id="CaMCa4" kdiff="4" kdiffunit="mu2/s" />
  
<!--Merging a stochastic model by  Keizer and Smith with CaM inhibition. We know that RyR2 has a much higher affinity for Apo-CaM than anything else and that ApoCaM inhibits RyR2 in very low cytosolic Ca  -->
<Reaction name = "RyRCaM2BD" id="RyRCaM2BD">
  <Reactant specieID="RyRD2D3"/>
  <Reactant specieID="CaM" power="4"/>
  <Product specieID="RyR4CaMD2D3"/>
  <forwardRate>3.33e-07</forwardRate>
  <reverseRate>2.702e-05</reverseRate>
</Reaction>

<Reaction name = "RyRCaM3BD" id="RyRCaM3BD">
  <Reactant specieID="RyRD2D3"/>
  <Reactant specieID="CaM" power="4"/>
  <Product specieID="RyRD24CaMD3"/>
  <forwardRate>3.33e-08</forwardRate>
  <reverseRate>2.702e-06</reverseRate>
 </Reaction>

<Reaction name = "RyRCaMCCa22BD" id="RyRCaMCCa22BD">
  <Reactant specieID="RyR4CaMD2D3"/>
  <Reactant specieID="Ca" n="8"/>
  <Product specieID="RyR4CaMCCa8D2D3"/> 
  <forwardRate>1.3e-21</forwardRate>
  <reverseRate>6.86e-9</reverseRate>
 </Reaction>

 <Reaction name = "RyR4CaMCCa822BD" id="RyR4CaMCCa82BD">
  <Reactant specieID="RyR4CaMCCa8D2D3"/>
  <Reactant specieID="Ca" n="8"/>
  <Product specieID="RyR4CaMCa16D2D3"/> 
  <forwardRate>1e-16</forwardRate>
  <reverseRate>1</reverseRate>
 </Reaction>

 

  <Reaction name = "RyRe" id="RyRe"> 
    <Reactant specieID="RyR4CaMCCa8D2D3"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="Ca4RyR4CaMCCa8D2D3"/>
    <forwardRate>5e-11</forwardRate>
    <reverseRate>50e-3</reverseRate>
  </Reaction>

  <Reaction name = "RyRe1" id="RyRe1"> 
    <Reactant specieID="RyR4CaMCa16D2D3"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="Ca4RyR4CaMCa16D2D3"/>
    <forwardRate>5e-11</forwardRate>
    <reverseRate>50e-3</reverseRate>
 </Reaction>

  <Reaction name = "RyRa" id="RyRa">
    <Reactant specieID="RyR4CaMCCa8D2D3"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyRO1"/>
    <forwardRate>5e-10</forwardRate>
    <reverseRate>9600e-3</reverseRate>
 </Reaction>
   
  <Reaction name = "RyRa1" id="RyRa1">
    <Reactant specieID="Ca4RyR4CaMCa16D2D3"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyRO2"/>
    <forwardRate>5e-10</forwardRate>
    <reverseRate>9600e-3</reverseRate>
  </Reaction>
  
  <Reaction name = "RyRb" id="RyRb">
    <Reactant specieID="RyRO1"/>
    <Reactant specieID="Ca" power="8"/>
    <Product specieID="RyRO2" />
    <forwardRate>1e-16</forwardRate>
    <reverseRate>1</reverseRate>
  </Reaction>

  <Reaction name = "RyRclamped1" id="RyRclamped1">
    <Reactant specieID="RyR4CaMCa16D2D3"/>
    <Product specieID="RyR4CaMCa16D2D3clamped"/>
    <forwardRate>5e-5</forwardRate>
    <reverseRate>5e-6</reverseRate>
  </Reaction>

  <Reaction name = "RyRclamped2" id="RyRclamped2">
    <Reactant specieID="Ca4RyR4CaMCa16D2D3"/>
    <Product specieID="Ca4RyR4CaMCa16D2D3clamped"/>
    <forwardRate>5e-5</forwardRate>
    <reverseRate>5e-6</reverseRate>
  </Reaction>

  <Reaction name = "RyRclamped3" id="RyRclamped3">
    <Reactant specieID="RyRO2"/>
    <Product specieID="Ca8RyR4CaMCa16D2D3clamped"/>
    <forwardRate>5e-5</forwardRate>
    <reverseRate>5e-6</reverseRate>
  </Reaction>

  <Reaction name = "RyReclamped" id="RyReclamped"> 
    <Reactant specieID="RyR4CaMCa16D2D3clamped"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="Ca4RyR4CaMCa16D2D3clamped"/>
    <forwardRate>5e-11</forwardRate>
    <reverseRate>50e-3</reverseRate>
 </Reaction>


 <Reaction name = "RyRcclamped" id="RyRcclamped"> 
    <Reactant specieID="Ca4RyR4CaMCa16D2D3clamped"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="Ca8RyR4CaMCa16D2D3clamped"/>
    <forwardRate>5e-11</forwardRate>
    <reverseRate>50e-3</reverseRate>
 </Reaction>

  <!--Sobie et al. 2002 (Saleet Jafri's) states 4/ms, Keizer and Levine 5/ms for small ER-->
   <Reaction name = "RyRflux1" id="RyRflux1">
     <Reactant specieID="RyRO1"/>
     <Reactant specieID="CaER"/>
     <Product specieID="RyRO1"/>
     <Product specieID="Ca"/>
    <forwardRate>5e-3</forwardRate>
    <reverseRate>5e-3</reverseRate>
  </Reaction>
  <Reaction name = "RyRflux2" id="RyRflux2">
     <Reactant specieID="RyRO2"/>
     <Reactant specieID="CaER"/>
     <Product specieID="RyRO2"/>
     <Product specieID="Ca"/>
    <forwardRate>5e-3</forwardRate>
    <reverseRate>5e-3</reverseRate>
  </Reaction>


 <!-- CaM, CaMCa4 and PP2B Binding -->
  <!--1) CaM  + 2Ca
      <-> CaMCa2 -->
  <!--This is the C site-->
  <Reaction name="CaMC_bind" id="CaMC_bind">
    <Reactant specieID="CaM" />
    <Reactant specieID="Ca" n="2" />
    <Product specieID="CaMCa2C" />
    <forwardRate>6e-6</forwardRate>
    <reverseRate>9.1e-3</reverseRate>
    <Q10>0.2</Q10>
  </Reaction>
  <!--2) CaMCa2  + 2Ca
      <-> CaMCa4 -->
  <!--This is the N-site-->
  <Reaction name="CaMCa2C_bind" id="CaMCa2C_bind">
    <Reactant specieID="CaMCa2C" />
    <Reactant specieID="Ca" n="2" />
    <Product specieID="CaMCa4" />
    <forwardRate>0.1e-3</forwardRate>
    <reverseRate>1000e-3</reverseRate>
    <Q10>0.2</Q10>
  </Reaction>
  <!--1B) CaM  + 2Ca <-> CaMCa2N GQ RXN FILE, C or N can bind first-->
  <Reaction name = "CaMN_bind" id="CaMN_bind">
    <Reactant specieID="CaM"/>
    <Reactant specieID="Ca" n="2"/>
    <Product specieID="CaMCa2N"/>
    
    <forwardRate>0.1e-3</forwardRate>
    <reverseRate>1000e-3</reverseRate>
    
  </Reaction>

  <!--2B) CaMCa2N  + 2Ca <-> CaMCa4 GQ RXN FILE, C or N can bind first-->
  <Reaction name = "CaMCa2N_bind" id="CaMCa2N_bind">
    <Reactant specieID="CaMCa2N"/>
    <Reactant specieID="Ca" n="2"/>
    <Product specieID="CaMCa4"/>
    
    <forwardRate>6e-6</forwardRate>
    <reverseRate>9.1e-3</reverseRate>
    
  </Reaction>


</ReactionScheme>
