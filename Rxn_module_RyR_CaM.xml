<?xml version="1.0"?>
<ReactionScheme>
  <Specie name="Ca" id="Ca" kdiff="100" kdiffunit="mu2/s" />
  <Specie name="CaER" id="CaER" kdiff="10." kdiffunit="mu2/s"/>
  
  <Specie name="RyRCaM"            id="RyRCaM"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR2CaM"            id="RyR2CaM"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3CaM"            id="RyR3CaM"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaM"            id="RyR4CaM"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRCaMCa2"            id="RyRCaMCa2"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR2CaMCa2"            id="RyR2CaMCa2"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3CaMCa2"            id="RyR3CaMCa2"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMCa2"            id="RyR4CaMCa2"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR2CaMCa4"            id="RyR2CaMCa4"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3CaMCa4"            id="RyR3CaMCa4"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMCa4"            id="RyR4CaMCa4"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3CaMCa6"            id="RyR3CaMCa6"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMCa6"            id="RyR4CaMCa6"           kdiff="0" kdiffunit="mu2/s"/>
  
  <Specie name="RyR4CaMCCa8"            id="RyR4CaMCCa8"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR4CaMCa16"            id="RyR4CaMCa16"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca4RyR4CaMCCa8"            id="Ca4RyR4CaMCCa8"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca4RyR4CaMCa16"            id="Ca4RyR4CaMCa16"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyRO1"            id="RyRO1"           kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="Ca8RyR4CaMCa16"            id="Ca8RyR4CaMCa16"           kdiff="0" kdiffunit="mu2/s"/>
    <Specie name="CaM" id="CaM" kdiff="4" kdiffunit="mu2/s" />
  <!--Calmodulin-->
  <Specie name="CaMCa2C" id="CaMCa2C" kdiff="4" kdiffunit="mu2/s" />
  <Specie name="CaMCa2N" id="CaMCa2N" kdiff="4" kdiffunit="mu2/s" />
  <!-- 2 Ca bound Calmodulin-->
  <Specie name="CaMCa4" id="CaMCa4" kdiff="4" kdiffunit="mu2/s" />
  
<!--Merging a stochastic model by  Keizer and Smith with CaM inhibition. We know that RyR2 has a much higher affinity for Apo-CaM than anything else and that ApoCaM inhibits RyR2 in very low cytosolic Ca  -->


<Reaction name = "RyRCaM2BD2" id="RyRCaM2BD2">
  <Reactant specieID="RyRCaM"/>
  <Reactant specieID="CaM" />
  <Product specieID="RyR2CaM"/>
  <forwardRate>6.66e-3</forwardRate>
  <reverseRate>4.8e-03</reverseRate>
</Reaction>

<Reaction name = "RyRCaM2BD3" id="RyRCaMBD3">
  <Reactant specieID="RyR2CaM"/>
  <Reactant specieID="CaM" />
  <Product specieID="RyR3CaM"/>
  <forwardRate>6.66e-3</forwardRate>
  <reverseRate>4.8e-03</reverseRate>
</Reaction>

<Reaction name = "RyRCaM2BD4" id="RyRCaMBD4">
  <Reactant specieID="RyR3CaM"/>
  <Reactant specieID="CaM" />
  <Product specieID="RyR4CaM"/>
  <forwardRate>6.66e-3</forwardRate>
  <reverseRate>4.8e-03</reverseRate>
</Reaction>

<Reaction name = "RyRCaMCCa22BD1" id="RyRCaMCCa22BD1">
  <Reactant specieID="RyRCaM"/>
  <Reactant  specieID="Ca" power="2"/>
  <Product specieID="RyRCaMCa2"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR2CaMCCa22BD1" id="RyR2CaMCCa22BD1">
  <Reactant specieID="RyR2CaM"/>
  <Reactant  specieID="Ca" power="2"/>
  <Product specieID="RyR2CaMCa2"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR3CaMCCa2BD1" id="RyR3CaMCCa22BD1">
  <Reactant specieID="RyR3CaM"/>
  <Reactant  specieID="Ca" power="2"/>
  <Product specieID="RyR3CaMCa2"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR4CaMCCa2BD1" id="RyR4CaMCCa22BD1">
  <Reactant specieID="RyR4CaM"/>
  <Reactant  specieID="Ca" power="2"/>
  <Product specieID="RyR4CaMCa2"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR2CaMCCa22BD2" id="RyR2CaMCCa22BD2">
  <Reactant specieID="RyR2CaMCa2"/>
  <Reactant  specieID="Ca" power="2"/>
  <Product specieID="RyR2CaMCa4"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR3CaMCCa2BD2" id="RyR3CaMCCa22BD2">
  <Reactant specieID="RyR3CaMCa2"/>
  <Reactant specieID="Ca" power="2"/>
  <Product specieID="RyR3CaMCa4"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR4CaMCCa2BD2" id="RyR4CaMCCa22BD2">
  <Reactant specieID="RyR4CaMCa2"/>
  <Reactant specieID="Ca" power="2"/>
  <Product specieID="RyR4CaMCa4"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

  <Reaction name = "RyR3CaMCCa2BD3" id="RyR3CaMCCa22BD3">
  <Reactant specieID="RyR3CaMCa4"/>
  <Reactant specieID="Ca" power="2"/>
  <Product specieID="RyR3CaMCa6"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR4CaMCCa2BD3" id="RyR4CaMCCa22BD3">
  <Reactant specieID="RyR4CaMCa4"/>
  <Reactant specieID="Ca" power="2"/>
  <Product specieID="RyR4CaMCa6"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>

 <Reaction name = "RyR4CaMCCa2BD4" id="RyR4CaMCCa22BD4">
  <Reactant specieID="RyR4CaMCa6"/>
  <Reactant specieID="Ca" power="2"/>
  <Product specieID="RyR4CaMCCa8"/> 
  <forwardRate>1e-7</forwardRate>
  <reverseRate>1e-2</reverseRate>
 </Reaction>


 
 <Reaction name = "RyR4CaMCCa822BD" id="RyR4CaMCCa82BD">
  <Reactant specieID="RyR4CaMCCa8"/>
  <Reactant specieID="Ca" power="8"/>
  <Product specieID="RyR4CaMCa16"/> 
  <forwardRate>1e-16</forwardRate>
  <reverseRate>1</reverseRate>
 </Reaction>

 

  <Reaction name = "RyRe" id="RyRe"> 
    <Reactant specieID="RyR4CaMCCa8"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="Ca4RyR4CaMCCa8"/>
    <forwardRate>5e-11</forwardRate>
    <reverseRate>50e-3</reverseRate>
  </Reaction>

  <Reaction name = "RyRe1" id="RyRe1"> 
    <Reactant specieID="RyR4CaMCa16"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="Ca4RyR4CaMCa16"/>
    <forwardRate>5e-11</forwardRate>
    <reverseRate>50e-3</reverseRate>
 </Reaction>

  <Reaction name = "RyRa" id="RyRa">
    <Reactant specieID="RyR4CaMCCa8"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyRO1"/>
    <forwardRate>5e-10</forwardRate>
    <reverseRate>9600e-3</reverseRate>
 </Reaction>
   
  <Reaction name = "RyRa1" id="RyRa1">
    <Reactant specieID="Ca4RyR4CaMCa16"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="Ca8RyR4CaMCa16"/>
    <forwardRate>5e-10</forwardRate>
    <reverseRate>9600e-3</reverseRate>
  </Reaction>
  
  <Reaction name = "RyRb" id="RyRb">
    <Reactant specieID="RyRO1"/>
